{
  "name": "icao.github.io",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Cesar Aparicio",
    "url": "https://github.com/icao"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn run eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "yarn run eslint . --ext js,jsx,ts,tsx --fix",
    "format": " npx prettier --check \"**/*.{js,jsx,json,ts,tsx,scss,css}\"",
    "format:fix": " npx prettier --write \"**/*.{js,jsx,json,ts,tsx,scss,css}\"",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "next": "^12.1.5",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.50.1",
    "sharp": "^0.30.4",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@hover/storybook-addon-pseudo-states": "^1.1.3",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.22",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "sass-loader": "^12.6.0",
    "storybook-addon-outline": "^1.4.2",
    "style-loader": "^3.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix --color",
    "*.{js,jsx,json,ts,tsx,scss,css}": "npx prettier --write --color"
  }
}
